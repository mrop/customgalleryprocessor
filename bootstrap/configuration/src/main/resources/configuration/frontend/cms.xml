<?xml version="1.0" encoding="UTF-8"?><sv:node xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:h="http://www.onehippo.org/jcr/xmlimport" sv:name="cms" h:merge="combine">
  <sv:node sv:name="cms-services" h:merge="combine">
    <sv:node sv:name="galleryProcessorService" h:merge="combine">
      <sv:property sv:name="plugin.class" sv:type="String" h:merge="override">
        <sv:value>org.example.frontend.gallery.imageutil.ScalingCroppingGalleryProcessorPlugin</sv:value>
      </sv:property>
      <sv:node sv:name="customgalleryprocessor:landscape">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>frontend:pluginconfig</sv:value>
        </sv:property>
        <sv:property sv:name="compression" sv:type="Double">
          <sv:value>1.0</sv:value>
        </sv:property>
        <sv:property sv:name="height" sv:type="Long">
          <sv:value>32</sv:value>
        </sv:property>
        <sv:property sv:name="optimize" sv:type="String">
          <sv:value>quality</sv:value>
        </sv:property>
        <sv:property sv:name="upscaling" sv:type="Boolean">
          <sv:value>false</sv:value>
        </sv:property>
        <sv:property sv:name="width" sv:type="Long">
          <sv:value>125</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="customgalleryprocessor:square">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>frontend:pluginconfig</sv:value>
        </sv:property>
        <sv:property sv:name="compression" sv:type="Double">
          <sv:value>1.0</sv:value>
        </sv:property>
        <sv:property sv:name="height" sv:type="Long">
          <sv:value>200</sv:value>
        </sv:property>
        <sv:property sv:name="optimize" sv:type="String">
          <sv:value>quality</sv:value>
        </sv:property>
        <sv:property sv:name="upscaling" sv:type="Boolean">
          <sv:value>false</sv:value>
        </sv:property>
        <sv:property sv:name="width" sv:type="Long">
          <sv:value>200</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="customgalleryprocessor:portrait">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>frontend:pluginconfig</sv:value>
        </sv:property>
        <sv:property sv:name="compression" sv:type="Double">
          <sv:value>1.0</sv:value>
        </sv:property>
        <sv:property sv:name="height" sv:type="Long">
          <sv:value>125</sv:value>
        </sv:property>
        <sv:property sv:name="optimize" sv:type="String">
          <sv:value>quality</sv:value>
        </sv:property>
        <sv:property sv:name="upscaling" sv:type="Boolean">
          <sv:value>false</sv:value>
        </sv:property>
        <sv:property sv:name="width" sv:type="Long">
          <sv:value>32</sv:value>
        </sv:property>
      </sv:node>
    </sv:node>
  </sv:node>
</sv:node>
